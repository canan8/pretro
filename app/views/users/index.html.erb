<h1 align="center">Users of x Team</h1>
<div align="center">
  <% @users.each do |user| %>
    <ul class="listing">
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <li>
            <%= link_to user.email, user %>
          </li>
        </div>
      </div>
    </ul>
  <% end %>
</div>